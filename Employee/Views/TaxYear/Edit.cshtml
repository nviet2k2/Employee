@model TaxYearEditViewModel

<div class="card">
    <div class="card-body">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-action="Index">All TaxYear</a></li>
                <li class="breadcrumb-item active" aria-current="page">Create TaxYear</li>
            </ol>
        </nav>
    </div>

    <form method="post" asp-action="Edit" enctype="multipart/form-data">
        <div asp-validation-summary="All" class="text-danger"></div>
        <div class="form-row">


            <div class="col-md-2"></div>
            <div class="form-group col-md-2">
                <label asp-for="YearOfTax"></label>
            </div>
            <div class="form-group col-md-3">
                <input asp-for="YearOfTax" class="form-control" />
                <span asp-validation-for="YearOfTax" class="text-danger"></span>
            </div>
        </div>


        <div class="form-group">

            <button type="submit" class="btn btn-rounded btn-primary float-left">
                <i class="fas fa-plus-square" style="margin-right:0.7em"></i>Save
            </button>
            <a asp-action="index" class="btn btn-rounded btn-success float-right"><i class="fas fa-caret-square-left" style="margin-right:0.7em"></i>Back</a>
            <br />
            <br />
            <br />

        </div>
    </form>

</div>

@section Scripts{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}